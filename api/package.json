  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci --maxWorkers=2",
    "build": "tsc --outDir out",
    "start": "node out/index.js",
    "dev": "ts-node index.ts",
    "lint": "cd .. && npm run lint",
    "lint:fix": "cd .. && npm run lint:fix",
    "lint:security": "cd .. && npm run lint:security",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:status": "sequelize-cli db:migrate:status",
    "migration:generate": "sequelize-cli migration:generate --name",
    "model:generate": "sequelize-cli model:generate --name",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo",
    "seed:undo:all": "sequelize-cli db:seed:undo:all",
    "seed:generate": "sequelize-cli seed:generate --name"
  },
